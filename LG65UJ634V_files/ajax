window.ttMETA=(typeof(window.ttMETA)!="undefined")?window.ttMETA:[];window.ttMETA.push({"CampaignName":"","CampaignId":"","RecipeName":"","RecipeId":"","OfferId":"","OfferName":"","MboxName":"wasPrice"});
});mboxCurrent.loaded();mboxFactories.get('default').get('wasPrice',0).cancelTimeout();mboxFactories.get('default').get('wasPrice',0).setOffer(new mboxOfferDefault()).show();